<html>
	<head>
		<script src="/js/d3.v2.min.js"></script>
		<script src="/js/d3.v3.js"></script>
		<script src="/js/nv.d3.min.js"></script>
		<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>

		<script>

			barData = [ 
			  {
			    key: "Comparison of Price and valuations",
			    values: <%- JSON.stringify(prices) %>
			  }
			];


			$(function() {
				nv.addGraph(function() {  
					var chart = nv.models.discreteBarChart()
					    .x(function(d) { return d.label })
					    .y(function(d) { return d.value })
					    .staggerLabels(true)
					    .tooltips(false)
					    .showValues(true)

					d3.select('#chart1 svg')
					    .datum(barData)
						.transition().duration(500)
					    .call(chart);

					nv.utils.windowResize(chart.update);

					return chart;
				});
			});
		</script>
	</head>
	<body>
		<table>
			<tr><td></td><td>Cash from Operations</td><td>Capital Expenditures</td><td>Dividends Paid</td><td>FCF</td></tr>
			<tr><td>2012</td><td><%= cashflow["Cash from Operating Activities"][0] %></td><td><%= cashflow["Capital Expenditures"][0] %></td><td><%= cashflow["Total Cash Dividends Paid"][0] %></td><td><%= FCF[0] %></td></tr>
			<tr><td>2011</td><td><%= cashflow["Cash from Operating Activities"][1] %></td><td><%= cashflow["Capital Expenditures"][1] %></td><td><%= cashflow["Total Cash Dividends Paid"][1] %></td><td><%= FCF[1] %></td></tr>
			<tr><td>2010</td><td><%= cashflow["Cash from Operating Activities"][2] %></td><td><%= cashflow["Capital Expenditures"][2] %></td><td><%= cashflow["Total Cash Dividends Paid"][2] %></td><td><%= FCF[2] %></td></tr>
			<tr><td>2009</td><td><%= cashflow["Cash from Operating Activities"][3] %></td><td><%= cashflow["Capital Expenditures"][3] %></td><td><%= cashflow["Total Cash Dividends Paid"][3] %></td><td><%= FCF[3] %></td></tr>
		</table>

		<div id="chart1" style="width:700px; height:250px;">
			<svg></svg>
		</div>

		<table>

			<tr><td>Present Value: </td><td>$<%= PV %></td></tr>
			<tr><td>Horizon Value: </td><td>$<%= HV %></td></tr>
			<tr><td>Intrinsic Value: </td><td>$<%= IV %></td></tr>

		</table><table>

			<tr><td>Total Revenue: </td><td>$<%= income["Total Revenue"][0] %></td></tr>
			<tr><td>Income After Tax: </td><td>$<%= income["Income After Tax"][0] %></td></tr>
			<tr><td>Profit Margin: </td><td><%= (profit_margin * 100).toFixed(2) %>%</td></tr>

		</table><table>

			<tr><td>Total Assets: </td><td>$<%= balances["Total Assets"][0] %></td></tr>
			<tr><td>Total Liabilities: </td><td>$<%= balances["Total Liabilities"][0] %></td></tr>
			<tr><td>Total Equity: </td><td>$<%= balances["Total Equity"][0] %></td></tr>

		</table><table>

			<tr><td>Shares Outstanding: </td><td><%= shares_outstanding %></td></tr>
			<tr><td>IV + Equity: </td><td>$<%= iv_equity %></td></tr>


		</table><table>

			<tr><td>1:1 Value: </td><td>$<%= one_to_one_value %></td></tr>
			<tr><td>2:1 Value: </td><td>$<%= two_to_one_value %></td></tr>
			<tr><td>3:1 Value: </td><td>$<%= three_to_one_value %></td></tr>
			<tr><td>1:2 Value: </td><td>$<%= one_to_two_value %></td></tr>
			<tr><td>1:3 Value: </td><td>$<%= one_to_three_value %></td></tr>

		</table>
	</body>
</html>